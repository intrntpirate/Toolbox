  <Code><![CDATA[param (
	$function = "Add",
	$RegPath
)
function Check
{
	$state = $false
	If ($textboxName.Text.Length -ge 1)
	{
		$state = $true
	}
	If ((Test-Path -Path "$($textboxExecute.Text)"))
	{
		$state = $true
		If (!($Script:UseCustomIcon -eq $true))
		{
			$Error.Clear()
			$picturebox1.Image = ([Drawing.Icon]::ExtractAssociatedIcon("$($textboxExecute.Text)").ToBitmap())
			If ($Error)
			{
				Log -text "An error occured while attempting to extract the icon." -Component "AddShortcut.psf_Check" -Type 3
				Log -text "$Error" -Component "AddShortcut.psf_Check" -Type 3
			}
		}
	}
	Else
	{
		$state = $false
	}
	$buttonAdd.Enabled = $state
	$buttonTest.Enabled = $state
}
Function Get-FileName($initialDirectory)
{
	[System.Reflection.Assembly]::LoadWithPartialName("System.windows.forms") | Out-Null
	$OpenFileDialog = New-Object System.Windows.Forms.OpenFileDialog
	$OpenFileDialog.DereferenceLinks = $false
	$OpenFileDialog.initialDirectory = $initialDirectory
	$OpenFileDialog.filter = "All files (*.*)| *.*"
	$OpenFileDialog.ShowDialog() | Out-Null
	$OpenFileDialog.filename
}
$formNewShortcut_Load={
	If ($function -eq "Add")
	{
		Check	
	}
	elseif ($function -eq "Edit")
	{
		Debug-Log -text "Loading in Edit view..." -Component "AddShortcut.psf_formNewShortcut_Load" -Type 4
		Debug-Log -text "Received the RegPath of: $RegPath" -Component "AddShortcut.psf_formNewShortcut_Load" -Type 4
		$buttonAdd.Text = "Save"
		$formNewShortcut.Text = "Edit Shortcut"
		$Script:Properties = Get-ItemProperty -Path "$RegPath"
		$textboxName.Text = "$($Properties.Name)"
		$textboxExecute.Text = "$($Properties.LocalPath)"
		If (($Properties.Args))
		{
			$textboxArgs.Text = "$($Properties.Args)"
		}
		If (($Properties.RunAs))
		{
			If ($Properties.RunAs -eq "YES")
			{
				$checkboxRunAsAdmin.Checked = $true
			}
			Else
			{
				$checkboxRunAsAdmin.Checked = $false
			}
		}
		If (($Properties.IconPath))
		{
			$Error.Clear()
			$picturebox1.Image = ([Drawing.Icon]::ExtractAssociatedIcon("$($Properties.IconPath)").ToBitmap())
			If ($Error)
			{
				Log -text "An error occured while attempting to extract the custom icon." -Component "AddShortcut.psf_formNewShortcut_Load" -Type 3
				Log -text "$Error" -Component "AddShortcut.psf_formNewShortcut_Load" -Type 3
			}
			$Script:CustomIcon = "$($Properties.IconPath)"
			$Script:UseCustomIcon = $true
			$buttonReset.Visible = $true
		}
		Else
		{
			$Script:UseCustomIcon = $false
			$Error.Clear()
			$picturebox1.Image = ([Drawing.Icon]::ExtractAssociatedIcon("$($Properties.LocalPath)").ToBitmap())
			If ($Error)
			{
				Log -text "An error occured while attempting to extract the default icon." -Component "AddShortcut.psf_formNewShortcut_Load" -Type 3
				Log -text "$Error" -Component "AddShortcut.psf_formNewShortcut_Load" -Type 3
			}
			$buttonReset.Visible = $false
		}
	}
}
$textboxName_TextChanged={
	Check
}
$textboxExecute_TextChanged={
	Check
}
$buttonBrowse_Click={
	$textboxExecute.Text = Get-FileName -initialDirectory $env:SystemDrive
	Check
}
$buttonAdd_Click={
	$formNewShortcut.Cursor = 'WaitCursor'
	If ($function -eq "Add")
	{
		Debug-Log -text "Processing Add for $($textboxName.Text)..." -Component "AddShortcut.psf_buttonAdd_Click" -Type 4
		If ((Test-Path -Path "$Config_RootHKCU$AppShortName\Shortcuts\$($textboxName.Text)\"))
		{
			Debug-Log -text "A registry path already exists for the name of this new tool. Unable to add/overwrite/re-use." -Component "AddShortcut.psf_buttonAdd_Click" -Type 4
			Show-MessageBox_psf -title "ERROR" -text "A shortcut with the name $($textboxName.Text) already exists." -boxtype 1 -image 5 -x ($formNewShortcut.Location.X + ($formNewShortcut.Size.Width / 2)) -y ($formNewShortcut.Location.Y + ($formNewShortcut.Size.Height / 2))
		}
		Else
		{
			Debug-Log -text "Verifying the Shortcuts root key exists..." -Component "AddShortcut.psf_buttonAdd_Click" -Type 4
			$Error.Clear()
			If (!(Test-Path -Path "$Config_RootHKCU$AppShortName\Shortcuts\"))
			{
				Debug-Log -text "Creating the Shortcuts root key..." -Component "AddShortcut.psf_buttonAdd_Click" -Type 4
				$Error.Clear()
				New-Item -Path "$Config_RootHKCU$AppShortName" -Name "Shortcuts"
			}
			If ($Error)
			{
				Log -text "An error occured while attempting to create the root registry key 'Shortcuts'." -Component "AddShortcut.psf_buttonAdd_Click" -Type 3
				Log -text "$Error" -Component "AddShortcut.psf_buttonAdd_Click" -Type 3
				Show-MessageBox_psf -title "ERROR" -text "An error occured while attempting to add the new shortcut.`n`nSee log for additional details." -boxtype 1 -image 5 -x ($formNewShortcut.Location.X + ($formNewShortcut.Size.Width / 2)) -y ($formNewShortcut.Location.Y + ($formNewShortcut.Size.Height / 2))
			}
			Else
			{
				
				Debug-Log -text "Creating Shortcut properties..." -Component "AddShortcut.psf_buttonAdd_Click" -Type 4
				$Error.Clear()
				New-Item -Path "$Config_RootHKCU$AppShortName\Shortcuts" -Name "$($textboxName.Text)"
				New-ItemProperty -Path "$Config_RootHKCU$AppShortName\Shortcuts\$($textboxName.Text)" -Name "Name" -Value "$($textboxName.Text)" -PropertyType "String"
				New-ItemProperty -Path "$Config_RootHKCU$AppShortName\Shortcuts\$($textboxName.Text)" -Name "LocalPath" -Value "$($textboxExecute.Text)" -PropertyType "String"
				If ($textboxArgs.Text.Length -ge 1)
				{
					New-ItemProperty -Path "$Config_RootHKCU$AppShortName\Shortcuts\$($textboxName.Text)" -Name "Args" -Value "$($textboxArgs.Text)" -PropertyType "String"
				}
				If ($checkboxRunAsAdmin.Checked -eq $true)
				{
					New-ItemProperty -Path "$Config_RootHKCU$AppShortName\Shortcuts\$($textboxName.Text)" -Name "RunAs" -Value "YES" -PropertyType "String"
				}
				Else
				{
					New-ItemProperty -Path "$Config_RootHKCU$AppShortName\Shortcuts\$($textboxName.Text)" -Name "RunAs" -Value "NO" -PropertyType "String"
				}
				If ($Script:UseCustomIcon -eq $true)
				{
					New-ItemProperty -Path "$Config_RootHKCU$AppShortName\Shortcuts\$($textboxName.Text)" -Name "IconPath" -Value "$CustomIcon" -PropertyType "String"
				}
				If ($Error)
				{
					Log -text "An error occured while attempting to write properties of the shortcut to the registry." -Component "AddShortcut.psf_buttonAdd_Click" -Type 3
					Log -text "$Error" -Component "AddShortcut.psf_buttonAdd_Click" -Type 3
					Show-MessageBox_psf -title "ERROR" -text "An error occured while attempting to add the new shortcut.`n`nSee log for additional details." -boxtype 1 -image 5 -x ($formNewShortcut.Location.X + ($formNewShortcut.Size.Width / 2)) -y ($formNewShortcut.Location.Y + ($formNewShortcut.Size.Height / 2))
				}
				Else
				{
					Debug-Log -text "Successfully created shortcut." -Component "AddShortcut.psf_buttonAdd_Click" -Type 4
				}
			}
		}
	}
	elseif ($function -eq "Edit")
	{
		Debug-Log -text "Processing Edit for $($Properties.Name)..." -Component "AddShortcut.psf_buttonAdd_Click" -Type 4
		If (!($Properties.Name -eq "$($textboxName.Text)"))
		{
			Debug-Log -text "Looks like the name changed. Ensuring new name isn't already in use."
			If ((Test-Path -Path "$Config_RootHKCU$AppShortName\Shortcuts\$($textboxName.Text)\"))
			{
				Debug-Log -text "The new name is already in use." -Component "AddShortcut.psf_buttonAdd_Click" -Type 4
				Show-MessageBox_psf -title "ERROR" -text "The shortcut name you're trying to use is already in use." -boxtype 1 -image 5 -x ($formNewShortcut.Location.X + ($formNewShortcut.Size.Width / 2)) -y ($formNewShortcut.Location.Y + ($formNewShortcut.Size.Height / 2))
			}
			Else
			{
				Debug-Log -text "Updating Name..." -Component "AddShortcut.psf_buttonAdd_Click" -Type 4
				$Error.Clear()
				Rename-Item -Path "$RegPath" -NewName "$($textboxName.Text)"
				If ($Error)
				{
					Log -text "An error occured while attempting to rename the key." -Component "AddShortcut.psf_buttonAdd_Click" -Type 3
					Log -text "$Error" -Component "AddShortcut.psf_buttonAdd_Click" -Type 3
				}
				Else
				{
					Debug-Log -text "Updating the RegPath value..." -Component "AddShortcut.psf_buttonAdd_Click" -Type 4
					$RegPath = "$Config_RootHKCU$AppShortName\Shortcuts\$($textboxName.Text)\"
					Debug-Log -text "Renaming the Name item..." -Component "AddShortcut.psf_buttonAdd_Click" -Type 4
					$Error.Clear()
					Set-ItemProperty -Path "$RegPath" -Name "Name" -Value "$($textboxName.Text)"
					If ($Error)
					{
						Log -text "An error occured while attempting to update the Name item." -Component "AddShortcut.psf_buttonAdd_Click" -Type 3
						Log -text "$Error" -Component "AddShortcut.psf_buttonAdd_Click" -Type 3
					}
				}
				
			}
		}
		Debug-Log -text "Updating LocalPath item..." -Component "AddShortcut.psf_buttonAdd_Click" -Type 4
		$Error.Clear()
		Set-ItemProperty -Path "$RegPath" -Name "LocalPath" -Value "$($textboxExecute.Text)"
		If ($Error)
		{
			Log -text "An error occured while attempting to update the LocalPath item." -Component "AddShortcut.psf_buttonAdd_Click" -Type 3
			Log -text "$Error" -Component "AddShortcut.psf_buttonAdd_Click" -Type 3
		}
		Debug-Log -text "Updating Args item..." -Component "AddShortcut.psf_buttonAdd_Click" -Type 4
		$Error.Clear()
		If ($textboxArgs.Text.Length -ge 1)
		{
			If (($Properties.Args))
			{
				Set-ItemProperty -Path "$RegPath" -Name "Args" -Value "$($textboxArgs.Text)"
			}
			Else
			{
				New-ItemProperty -Path "$RegPath" -Name "Args" -Value "$($textboxArgs.Text)"
			}
		}
		Else
		{
			If (($Properties.Args))
			{
				Remove-ItemProperty -Path "$RegPath" -Name "Args" -Force
			}
		}
		If ($checkboxRunAsAdmin.Checked -eq $true)
		{
			Set-ItemProperty -Path "$Config_RootHKCU$AppShortName\Shortcuts\$($textboxName.Text)" -Name "RunAs" -Value "YES"
		}
		Else
		{
			Set-ItemProperty -Path "$Config_RootHKCU$AppShortName\Shortcuts\$($textboxName.Text)" -Name "RunAs" -Value "NO"
		}
		If ($Error)
		{
			Log -text "An error occured while attempting to update the Args item." -Component "AddShortcut.psf_buttonAdd_Click" -Type 3
			Log -text "$Error" -Component "AddShortcut.psf_buttonAdd_Click" -Type 3
		}
		Debug-Log -text "Updating IconPath item..." -Component "AddShortcut.psf_buttonAdd_Click" -Type 4
		$Error.Clear()
		If ($UseCustomIcon -eq $true)
		{
			If (($Properties.IconPath))
			{
				Set-ItemProperty -Path "$RegPath" -Name "IconPath" -Value "$CustomIcon"
			}
			Else
			{
				New-ItemProperty -Path "$RegPath" -Name "IconPath" -Value "$CustomIcon"
			}
		}
		Else
		{
			If (($Properties.IconPath))
			{
				Remove-ItemProperty -Path "$RegPath" -Name "IconPath" -Force
			}
		}
		If ($Error)
		{
			Log -text "An error occured while attempting to update the IconPath item." -Component "AddShortcut.psf_buttonAdd_Click" -Type 3
			Log -text "$Error" -Component "AddShortcut.psf_buttonAdd_Click" -Type 3
		}
	}
	$formNewShortcut.Cursor = 'Default'
	$Global:ReScanApps = $true
	$formNewShortcut.Close()
}
$buttonTest_Click={
	If ($textboxArgs.Text.Length -ge 1)
	{
		If ($checkboxRunAsAdmin.Checked -eq $true)
		{
			Start-Process -FilePath "$($textboxExecute.Text)" -ArgumentList "$($textboxArgs.Text)" -verb runas
		}
		Else
		{
			Start-Process -FilePath "$($textboxExecute.Text)" -ArgumentList "$($textboxArgs.Text)"		
		}
	}
	Else
	{
		If ($checkboxRunAsAdmin.Checked -eq $true)
		{
			Start-Process -FilePath "$($textboxExecute.Text)" -Verb runas
		}
		Else
		{
			Start-Process -FilePath "$($textboxExecute.Text)"
		}
	}
}
$buttonBrowseIcon_Click={
	Debug-Log -text "The custom icon Browse button was clicked." -Component "AddShortcut.psf_buttonBrowseIcon_Click" -Type 4
	Function Get-FileName($initialDirectory)
	{
		$OpenFileDialog = New-Object System.Windows.Forms.OpenFileDialog
		$OpenFileDialog.DereferenceLinks = $false
		$OpenFileDialog.initialDirectory = $initialDirectory
		$OpenFileDialog.ShowDialog() | Out-Null
		$OpenFileDialog.filename
	}
	$CustomIconPath = Get-FileName -initialDirectory "C:\"
	If ((Test-Path $CustomIconPath))
	{
		Debug-Log -text "Received custom icon path: $CustomIconPath" -Component "AddShortcut.psf_buttonBrowseIcon_Click" -Type 4
		$picturebox1.Image = ([Drawing.Icon]::ExtractAssociatedIcon("$CustomIconPath").ToBitmap())
		$Script:CustomIcon = $CustomIconPath
		$Script:UseCustomIcon = $true
		$buttonReset.Visible = $true
	}
	Else
	{
		Debug-Log -text "No path received." -Component "AddShortcut.psf_buttonBrowseIcon_Click" -Type 4
	}
}
$buttonReset_Click={
	Debug-Log -text "Resetting icon back to the default icon..." -Component "AddShortcut.psf_buttonReset_Click" -Type 4
	$Error.Clear()
	$picturebox1.Image = ([Drawing.Icon]::ExtractAssociatedIcon("$($textboxExecute.Text)").ToBitmap())
	If ($Error)
	{
		Log -text "An error occured while attempting to extract the icon." -Component "AddShortcut.psf_buttonReset_Click" -Type 3
		Log -text "$Error" -Component "AddShortcut.psf_buttonReset_Click" -Type 3
	}
	$Script:UseCustomIcon = $false
	Clear-Variable -Scope Script -Name CustomIcon
	$buttonReset.Visible = $false
}]]></Code>
